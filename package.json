{
  "name": "scoring-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "doppler run -- vite --port 3000",
    "build": "tsc -b && doppler run -- vite build",
    "build:prod": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --port 5173 --host",
    "build:preview": "vite build && vite preview",
    "test": "vitest run",
    "test:watch": "vitest --reporter verbose --watch",
    "test:coverage": "vitest run --reporter verbose --coverage",
    "e2e": "cypress run --browser chromium",
    "e2e:open": "cypress open --browser chromium"
  },
  "proxy": "http://localhost:8080",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "bootstrap-icons": "^1.11.3",
    "fast-xml-parser": "^5.2.5",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "vite": "^5.3.6"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/browser": "^3.2.4",
    "cypress": "^14.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.1",
    "sass": "^1.77.8",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4"
  }
}
